{
  "manifest_version": 2,
  "name": "SeriesGuide Chrome v2 - 'New Tab' mode",
  "short_name": "SeriesGuide Chrome",  
  "version" : "0.19",
  "description": "Manage your favorite TV series and movies from your browser's new tab page",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "topSites",
    "notifications" 
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }, 
  "homepage_url": "http://reddit.com/r/seriesguide/"
}