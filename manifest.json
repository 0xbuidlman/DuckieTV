{
  "manifest_version": 2,
  "name": "SeriesGudie Chrome v2",
  "description": "Manage and download you favorite series and movies from your browser's new tab page",
  "version": "0.1",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "topSites"
  ],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }
}